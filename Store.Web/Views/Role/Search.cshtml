
@model List<Store.Model.Models.Roles>
@{
    ViewBag.Title = "Search List";
}

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Role Name </th>
        </tr>
    </thead>
    <tbody>

       
        @if(Model.Count() == 0)
            {
                <h1>no results</h1>
            }
        else {
        foreach (var role in Model)
        {
            <tr>
                <td>@role.ID</td>
                <td>@role.RoleName</td>
                <td><a href="http://localhost:3523/Role/View/@role.ID" class="btn btn-primary">View</a></td>


            </tr>
        }
        }
    </tbody>
</table>