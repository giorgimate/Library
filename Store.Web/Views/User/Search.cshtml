
@model List<Store.Model.Models.Users>
@{
    ViewBag.Title = "Search List";
}

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Password</th>
            <th>User Role Title</th>
        </tr>
    </thead>
    <tbody>

       
        @if(Model.Count() == 0)
            {
                <h1>no results</h1>
            }
        else {
        foreach (var user in Model)
        {
            <tr>
                <td>@user.ID</td>
                <td>@user.UserName</td>
                <td>@user.PassWord</td>
                <td>@user.UserRoleTitle</td>
                @*<td>@user.Address</td>
                <td>@user.Email</td>
                <td>@user.PhoneNumber</td>
                <td>@user.HireDate</td>
                <td>@user.Sallary</td>*@
                <td><a href="http://localhost:3523/User/View/@user.ID" class="btn btn-primary">View</a></td>


            </tr>
        }
        }
    </tbody>
</table>